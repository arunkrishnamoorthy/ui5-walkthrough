<mvc:View xmlns:mvc="sap.ui.core.mvc"
      xmlns="sap.m"
      controllerName="ui5.walkthrough.controller.main">
    <Panel headerText="{i18n>panelTitle}">
        <content>
            <Input value="{/name}" description="{i18n>description} {/name}" />
            <Button text="{i18n>buttonText}" press=".onPressDisplay"></Button>
        </content>    
    </Panel>
</mvc:View>

